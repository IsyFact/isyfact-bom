<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
    <modelVersion>4.0.0</modelVersion>

    <groupId>de.bund.bva.isyfact</groupId>
    <artifactId>isyfact-bom</artifactId>
    <packaging>pom</packaging>
    <version>1.3.4</version>
    <name>IsyFact BOM</name>
    <description>Definiert die Versionen aller Bibliotheken der IsyFact-Basis und IsyFact-Standard Komponenten sowie die genutzten Produkte.</description>

    <parent>
        <groupId>de.bund.bva.isyfact</groupId>
        <artifactId>isyfact-masterpom-lib</artifactId>
        <version>1.3.2</version>
    </parent>

    <dependencyManagement>
        <dependencies>
            <!-- Basiskomponenten der IsyFact. -->
            <dependency>
                <groupId>de.bund.bva.isyfact</groupId>
                <artifactId>isyfact-base-bom</artifactId>
                <version>1.4.1</version>
                <type>pom</type>
                <scope>import</scope>
            </dependency>

            <!-- Standardkomponente der IsyFact. -->
            <dependency>
                <groupId>de.bund.bva.pliscommon</groupId>
                <artifactId>plis-batchrahmen</artifactId>
                <version>1.6.0</version>
            </dependency>
            <dependency>
                <groupId>de.bund.bva.pliscommon</groupId>
                <artifactId>plis-persistence</artifactId>
                <version>1.4.3</version>
            </dependency>
            <dependency>
                <groupId>de.bund.bva.pliscommon</groupId>
                <artifactId>plis-polling</artifactId>
                <version>1.2.3</version>
            </dependency>
            <dependency>
                <groupId>de.bund.bva.pliscommon</groupId>
                <artifactId>plis-serviceapi-core</artifactId>
                <version>1.4.4</version>
            </dependency>
            <dependency>
                <groupId>de.bund.bva.pliscommon</groupId>
                <artifactId>plis-serviceapi-sst</artifactId>
                <version>1.4.3</version>
            </dependency>
            <dependency>
                <groupId>de.bund.bva.pliscommon</groupId>
                <artifactId>plis-ueberwachung</artifactId>
                <version>1.4.4</version>
            </dependency>
            <dependency>
                <groupId>de.bund.bva.pliscommon</groupId>
                <artifactId>plis-web</artifactId>
                <version>4.1.2</version>
            </dependency>
        </dependencies>
    </dependencyManagement>

</project>